<% if students.present? %>
  <h6>Select Students</h6>
  <div class="row">
    <% students.each do |st|%>
      <div class="col-3 mt-2">
        <div class="form-check">
          <input class="form-check-input" type="checkbox" value="<%=st.id%>" id="student<%=st.id%>">
          <label class="form-check-label" for="student<%=st.id%>">
            <%= st.name%>
          </label>
        </div>
      </div>
    <%end%>
  </div>
<%end%>
